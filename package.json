{
  "name": "duplicate-file-finder",
  "version": "1.0.0",
  "description": "A tool to find and manage duplicate files",
  "main": "electron/main.cjs",
  "scripts": {
    "start": "concurrently \"npm run dev:server\" \"npm run electron-dev\"",
    "dev": "concurrently \"npm run electron\" \"npm run build:watch\"",
    "dev:server": "vite --port 5173",
    "electron": "electron .",
    "electron-dev": "electron .",
    "build": "tsc -p tsconfig.json && webpack --mode production",
    "build:watch": "webpack --watch --mode development",
    "dev:web": "webpack serve --mode development",
    "test": "jest",
    "electron-build": "electron-builder --win --x64",
    "electron-pack": "electron-packager . --platform=win32 --arch=x64 --out=dist --overwrite"
  },
  "keywords": [
    "duplicate",
    "file",
    "finder",
    "cleaner"
  ],
  "author": "VIP_APP",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "css-loader": "^6.9.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.12"
  }
}