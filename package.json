{
  "name": "duplicate-file-finder",
  "version": "1.0.0",
  "description": "A tool to find and manage duplicate files",
  "main": "electron/main.cjs",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run electron\" \"npm run build:watch\"",
    "electron": "electron .",
    "build": "tsc -p tsconfig.json && webpack --mode production",
    "build:watch": "webpack --watch --mode development",
    "dev:web": "webpack serve --mode development",
    "test": "jest"
  },
  "keywords": [
    "duplicate",
    "file",
    "finder",
    "cleaner"
  ],
  "author": "VIP_APP",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^7.0.0",
    "electron": "^22.0.0",
    "html-webpack-plugin": "^5.5.0",
    "ts-loader": "^9.4.0",
    "typescript": "^4.9.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "css-loader": "^6.7.1",
    "style-loader": "^3.3.1",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}